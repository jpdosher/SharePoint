{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "welcome-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/welcome/WelcomeWebPart.js",
          "manifest": "./src/webparts/welcome/WelcomeWebPart.manifest.json"
        }
      ]
    },
    "viva-test-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/vivaTest/VivaTestWebPart.js",
          "manifest": "./src/webparts/vivaTest/VivaTestWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "WelcomeWebPartStrings": "lib/webparts/welcome/loc/{locale}.js",
    "VivaTestWebPartStrings": "lib/webparts/vivaTest/loc/{locale}.js"
  }
}
